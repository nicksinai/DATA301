<section id="fourth" class="section-wrap padded tab-bottom-border" data-section="Resources">
    <h2 class="font-Museo-Sans-100 text-center padded">Resources</h2>
    
<div id="cs-tab" class="tabs-default tab-num-5">
    
    <nav>
        <ul role="tablist">
            <li id="ch1Tab" role="tab" aria-controls="panel1" tabindex="0" class="" aria-selected="false"><a href="#section-1" tabindex="-1"><i class="fa fa-rocket" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="Start A Club"></i><span>Start A Club</span></a></li>
